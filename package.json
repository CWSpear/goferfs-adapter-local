{
  "name": "goferfs-adapter-local",
  "version": "1.0.0-3",
  "description": "Local Adapter for Gofer",
  "main": "src/index.js",
  "types": "src/index.ts",
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "lint": "tslint src/**/*.ts",
    "clean": "rimraf src/**/*.js",
    "build": "npm run clean && npm run lint && tsc && babel -d . src/**/*.js",
    "test": "npm run build && mocha --require ts-node/register src/**/*.test.ts",
    "prepublish": "npm test"
  },
  "repository": {
    "url": "http://github.com/cohesivelabs/goferfs",
    "type": "git"
  },
  "author": "Cameron Spear <cam@cameronspear.com>",
  "contributors": [
    "Justen Martin <jmart@the-coder.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "bluebird": "^3.4.7",
    "fs-extra-promise": "^0.4.1",
    "goferfs-types": "^1.0.0-8",
    "mime": "^1.3.4"
  },
  "devDependencies": {
    "@types/fs-extra-promise": "^0.0.31",
    "@types/mime": "^0.0.29",
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.4",
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "goferfs-test-suite": "^1.0.0-9",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "ts-node": "^2.0.0",
    "tslint": "^4.4.2",
    "typescript": "^2.1.5"
  },
  "peerDependencies": {
    "goferfs": "^1.0.0-1"
  }
}
