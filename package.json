    "mime": "^1.3.4"
  },
  "devDependencies": {
    "@types/fs-extra-promise": "^0.0.31",
    "@types/mime": "^0.0.29",
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.4",
    "goferfs-test-suite": "^1.0.0-3",
    "mocha": "^3.2.0",
